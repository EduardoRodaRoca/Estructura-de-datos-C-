fecha = input("Ingrese su fecha de nacimiento en formato dd/mm/aaaa: ")
if fecha[1] == '/':
    dia = int(fecha[0])
    mes = int(fecha[2:4])
    anio = int(fecha[5:9])
elif fecha[2] == '/':
    dia = int(fecha[0:2])
    mes = int(fecha[3])
    anio = int(fecha[5:9])
else:
    dia = int(fecha[0:2])
    mes = int(fecha[3:5])
    anio = int(fecha[6:10]) 
print("Dia:", dia)
print("Mes:", mes)
print("Anio:", anio)
